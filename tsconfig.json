{
  "compilerOptions": {
    "lib": ["dom", "es2017"],
    "target": "ES2017",
    "module": "es2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "useDefineForClassFields": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "strictNullChecks": true
  },
  "vueCompilerOptions": {
    "target": 2.7
  }
}
